// 工具函数抽取

// 手机号判断
const phoneCheck=function(phone,success=()=>{},error=()=>{}){
	let myreg = /^(0|86|17951)?(13[0-9]|15[012356789]|166|17[3678]|18[0-9]|14[57])[0-9]{8}$/;
	if(myreg.test(phone)){
		success();
	}else{
		error();
	}
}
// 生成8位随机数
const generate8=function(){
	let i=Math.random()*100000000;
	i=i.toFixed(0);
	return i;
}


export default{
	phoneCheck,
	generate8
}